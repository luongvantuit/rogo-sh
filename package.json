{
  "name": "rogo-sh",
  "productName": "rogo-sh",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "author": "Lương Văn Tú",
  "keywords": [],
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "luongvantuit",
              "name": "rogo-sh"
            }
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "rogo-sh"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "connect-src 'self' https://hotel.rogo.com.vn ws://hotel.rogo.com.vn:8083/mqtt https://fonts.googleapis.com https://cdnjs.cloudflare.com https://identitytoolkit.googleapis.com https://securetoken.googleapis.com  'unsafe-eval'",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "6.0.0-beta.61",
    "@electron-forge/publisher-github": "^6.0.0-beta.63",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.0.0",
    "electron": "^19.0.1",
    "node-loader": "^2.0.0",
    "postcss": "^8.4.13",
    "style-loader": "^3.0.0",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "firebase": "^9.8.1",
    "https-browserify": "^1.0.0",
    "paho-mqtt": "^1.1.0",
    "path-browserify": "^1.0.1",
    "postcss-loader": "^6.2.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-qr-code": "^2.0.7",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "webpack": "^5.72.1"
  }
}
